{% extends "base_compta.html.twig" %} {% block title %} Ajouter cycle {% endblock
%} {% block style %}
<link
  href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css"
  rel="stylesheet"
/>
<script
  src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.8/js/select2.min.js"
  defer
></script>
{% endblock %} {% block body %}
<!-- Page Wrapper -->
<div id="wrapper">
  <!-- Sidebar -->
  {{ include("./includes/sidebar.html.twig") }}
  <!-- End of Sidebar -->

  <!-- Content Wrapper -->
  <div id="content-wrapper" class="d-flex flex-column">
    <!-- Main Content -->
    <div id="content">
      <!-- Topbar -->
      {{ include("./includes/navbar-compta.html.twig") }}
      <!-- End of Topbar -->

      <!-- Begin Page Content -->
      <div class="container-fluid">
        <!-- Page Heading -->
        <div class="row d-flex justify-content-center">
          <div class="col-lg-6 col-md-6 col-auto">
            {{ form_start(form) }}
            <h1 class="text-uppercase text-center">Ajouter cycle</h1>
            <div class="mb-3">
              <label for="">Nom cyle</label>
              {{
                form_row(form.nomCycle, {
                  label: false,
                })
              }}
            </div>
            <div class="mb-3">
              <label for="">date de début</label>
              {{
                form_row(form.dateDeb, {
                  label: false,
                })
              }}
            </div>
            <div class="mb-3">
              <label for="">date de fin</label>
              {{
                form_row(form.dateFin, {
                  label: false,
                })
              }}
            </div>
            <div class="mb-3">
              <label for="">commentaire</label>
              {{
                form_row(form.commentaire, {
                  label: false,
                })
              }}
            </div>
            <div class="mb-3">
              <button type="submit" class="btn btn-primary w-100">
                Submit
              </button>
            </div>
            {{ form_end(form) }}
          </div>
        </div>
      </div>
      <!-- /.container-fluid -->
    </div>
    <!-- End of Main Content -->

    <!-- Footer -->
    {{ include("./includes/footer-compta.html.twig") }}
    <!-- End of Footer -->
  </div>
  <!-- End of Content Wrapper -->
</div>
<!-- End of Page Wrapper -->

<!-- Scroll to Top Button-->
<a class="scroll-to-top rounded" href="#page-top">
  <i class="fas fa-angle-up"></i>
</a>

<!-- Logout Modal-->
<div
  class="modal fade"
  id="logoutModal"
  tabindex="-1"
  role="dialog"
  aria-labelledby="exampleModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
        <button
          class="close"
          type="button"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">×</span>
        </button>
      </div>
      <div class="modal-body">
        Select "Logout" below if you are ready to end your current session.
      </div>
      <div class="modal-footer">
        <button class="btn btn-secondary" type="button" data-dismiss="modal">
          Cancel
        </button>
        <a class="btn btn-primary" href="login.html">Logout</a>
      </div>
    </div>
  </div>
</div>

{% endblock %} {% block javascript %}

<script src="{{ asset('espace-compta/') }}vendor/jquery/jquery.min.js"></script>
<script src="{{
    asset('espace-compta/')
  }}vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- Core plugin JavaScript-->
<script src="{{
    asset('espace-compta/')
  }}vendor/jquery-easing/jquery.easing.min.js"></script>

<!-- Custom scripts for all pages-->
<script src="{{ asset('espace-compta/') }}js/sb-admin-2.min.js"></script>

<!-- Page level plugins -->
<script src="{{
    asset('espace-compta/')
  }}vendor/datatables/jquery.dataTables.min.js"></script>
<script src="{{
    asset('espace-compta/')
  }}vendor/datatables/dataTables.bootstrap4.min.js"></script>

<!-- Page level custom scripts -->
<script src="{{ asset('espace-compta/') }}js/demo/datatables-demo.js"></script>
