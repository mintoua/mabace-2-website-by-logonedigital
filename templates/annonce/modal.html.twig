<div class="modal fade" id="createModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                {{ form_start(form,{'action':url }) }}
                <div class="modal-body">
                    <div class="form-group">
                        <label for="name">Nom</label>
                        {{ form_widget(form.nom,{'attr': {'placeholder': 'Nom'}}) }}
                    </div>
                    <div class="form-group">
                        <label for="name">Prénom</label>
                        {{ form_widget(form.prenom,{'attr': {'placeholder': 'Prénom'}}) }}
                    </div>
                    <div class="form-group">
                        <label for="email">Email</label>
                        {{ form_widget(form.email,{'attr': {'placeholder': 'Email'}}) }}
                    </div>
                    <div class="form-group">
                        <label for="tel">Numéro de téléphone</label>
                        {{ form_widget(form.tel,{'attr': {'placeholder': 'Tel'}}) }}
                    </div>
                    <div class="form-check">
                        {{ form_widget(form.rgpd,{'attr': {'class': 'form-check-input'}}) }}
                        <label class="form-check-label" for="defaultCheck1">
                            J'accepte les conditions générales d'utilisations
                        </label>
                    </div>
                    <div class="form-group">


                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="button button-radius button-danger" data-dismiss="modal">Fermer</button>
                    <button type="submit" class="button button-xs button-radius button-outline-dark-2">Soumettre</button>
                </div>
                {{ form_end(form) }}

            </div>
        </div>
    </div>
</div>
