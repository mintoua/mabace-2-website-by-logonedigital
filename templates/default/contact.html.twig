<!-- Contact section -->
<div class="section">
  <div class="container">
    <div class="row g-4">
      <div class="col-12 col-md-4">
        <div class="margin-bottom-30">
          <h6 class="fw-normal margin-0">Phone:</h6>
          <p>+(987) 654 321 01</p>
        </div>
        <div class="margin-bottom-30">
          <h6 class="fw-normal margin-0">Email:</h6>
          <p>contact@email.com</p>
        </div>
        <div>
          <h6 class="fw-normal margin-0">Address:</h6>
          <p>121 King St, Melbourne VIC 3000</p>
        </div>
      </div>
      <div class="col-12 col-md-8">
        <div class="contact-form text-md-end">
          {{ form_start(form) }}
            <div class="row gx-3 gy-0">
              <div class="col-12 col-sm-6">
                {{
                form_row(form.nom, {
                  label: false,
                  attr: {
                    class: "form-input",
                    placeholder: "Nom et Pr√©nom"
                  }
                })
                }}
              </div>
              <div class="col-12 col-sm-6">
                {{
                form_row(form.email, {
                  label: false,
                  attr: {
                    class: "form-input",
                    placeholder: "Email"
                  }
                })
                }}
              </div>
            </div>
            {{
            form_row(form.message, {
              label: false,
              attr: {
                placeholder: "Laissez un message..."
              }
            })
            }}
            <button
              class="button button-lg button-radius button-outline-dark-2"
              type="submit"
            >
              Send Message
            </button>
          {{ form_end(form) }}
          <!-- Submit result -->
          <div class="submit-result">
            <span id="success">Thank you! Your Message has been sent.</span>
            <span id="error">Something went wrong, Please try again!</span>
          </div>
        </div>
      </div>
    </div>
    <!-- end row -->
  </div>
  <!-- end container -->
</div>
<!-- end Contact section -->

